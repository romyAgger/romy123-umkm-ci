{
    "name": "codeigniter4/umkm-crud",
    "type": "project",
    "description": "UMKM CRUD CodeIgniter4 Project",
    "keywords": ["framework", "codeigniter4", "umkm", "crud"],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "codeigniter4/framework": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "phpunit/phpunit": "^10.3",
        "codeigniter4/devkit": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "CodeIgniter\\ComposerScripts::postUpdate"
        ],
        "post-install-cmd": [
            "CodeIgniter\\ComposerScripts::postInstall"
        ],
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
